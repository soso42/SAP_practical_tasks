{
  "xsappname" : "xsuaa-service",
  "tenant-mode" : "shared",
  "scopes": [
    {
      "name": "$XSAPPNAME.Display",
      "description": "Display content"
    }
  ],
  "role-templates": [
    {
      "name": "Viewer",
      "description": "View content",
      "scope-references": [
        "$XSAPPNAME.Display"
      ]
    }
  ],
  "oauth2-configuration": {
    "redirect-uris": [
      "https://*.cfapps.us10-001.hana.ondemand.com/**"
    ]
  }
}
